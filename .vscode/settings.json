{
    "latex-workshop.latex.tools": [
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "--interaction=nonstopmode",
                "--synctex=1",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--max-print-line=10000",
                "--interaction=nonstopmode",
                "--synctex=1",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "LuaLaTeX",
            "tools": [
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "pdfLaTeX",
            "tools": [
                "pdflatex",
                "pdflatex"
            ]
        }
    ],
    "latex-workshop.latex.recipe.default": "LuaLaTeX"
}
